{
  "name": "wearesho-team/recaptcha-v3",
  "description": "reCAPTCHA v3 integration",
  "type": "library",
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^6.3",
    "horat1us/environment-config": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "squizlabs/php_codesniffer": "^3.3.2"
  },
  "autoload": {
    "psr-4": {
      "Wearesho\\ReCaptcha\\V3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\ReCaptcha\\V3\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "process-timeout": 1800,
    "sort-packages": true,
    "platform": {
      "php": "7.1"
    }
  },
  "scripts": {
    "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit"
  }
}
